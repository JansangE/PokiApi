<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- ICONS -->
    <Style x:Key="SearchIcon" TargetType="{x:Type Image}">
        <Setter Property="Source" Value="search(1).png"/>
        <Setter Property="Margin" Value="0"/>
    </Style>

    <Style x:Key="MainPhoto" TargetType="{x:Type Image}">
        <Setter Property="Source" Value="question.jpg"/>
    </Style>

    <Style x:Key="PokeballRed" TargetType="{x:Type Image}">
        <Setter Property="Source" Value="pokeball.png"/>
        <Setter Property="Width" Value="50"></Setter>
    </Style>
</ResourceDictionary>